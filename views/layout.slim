  body
    /! header-11
    header.header-11
      .container
        .navbar.span12
          .navbar-inner
            button.btn.btn-navbar type="button"
            a.brand href="/"
              img alt="" height="50" src="/img/logo@2x.png" width="50"
              | NinjaShipper
            .nav-collapse.collapse.pull-right
              ul.nav
                li class=( @page == :index ? 'active' : '' )
                  a href="/" Home
                li class=( @page == :service ? 'active' : '' )
                  a href="/service.html" Service
                li class=( @page == :prices ? 'active' : '' )
                  a href="/prices.html" Prices
                li class=( @page == :faq ? 'active' : '' )
                  a href="/faq.html" FAQ
                li
                  a href="http://member.ninjashipper.com/users/login" Sing in
              form.navbar-form.pull-left
                a.btn.btn-primary href="http://member.ninjashipper.com/users/add" Sign up
    == yield
    /! content-13
    section.content-13.subscribe-form.bg-turquoise.footer
      .container
        .row
          form
            .span8
              span Sign up for free
              /input placeholder="Enter your e-mail" spellcheck="false" type="text"
            .span4
              a.btn.btn-large.btn-primary href="http://member.ninjashipper.com/users/add" Sign up
    /! footer-2
    footer.footer-2.bg-midnight-blue
      .container
        nav.pull-left
          ul
            li.active
              a href="/" Home
            li
              a href="/service.html" Service
            li
              a href="/prices.html" Prices
            li
              a href="/faq.html" FAQ
            li
              a href="http://extendsandlvr.freshdesk.com/support/home" Contact
            li
              a href="http://member.ninjashipper.com/users/login" Sing in
            li
              a href="http://member.ninjashipper.com/users/add" Sing up for free
        .social-btns.pull-right
          /.fui-vimeo
          /  a href="#"
          /.fui-facebook
          /  a href="#"
          /.fui-twitter
          /  a href="#"
        .additional-links
          a href="privacy.html" Privacy Policy
          |  / 
          a href="company.html" Act on Specified Commercial Transaction
